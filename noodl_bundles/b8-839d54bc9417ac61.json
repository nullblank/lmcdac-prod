ek = days[date.getDay()];\nconst day = date.getDate();\nconst month = date.toLocaleString('default', { month: 'long' });\nconst year = date.getFullYear();\nlet hours = date.getHours();\nconst minutes = date.getMinutes();\nconst seconds = date.getSeconds();\nlet period = 'AM';\n\nif (hours >= 12) {\n    period = 'PM';\n    if (hours > 12) {\n        hours -= 12;\n    }\n}\n\nOutputs.Timestamp = dayOfWeek + ', ' + day + ' ' + month + ' ' + year + ' ' + hours + ':' + minutes + ':' + seconds + ' ' + period;\n"},"ports":[{"name":"in-Timestamp","displayName":"Timestamp","plug":"input","type":"*","group":"Inputs","index":4},{"name":"out-Timestamp","displayName":"Timestamp","plug":"output","type":"*","group":"Outputs","index":5}],"children":[]},{"id":"8c868565-a3bf-5cbd-2544-547b58c592f1","type":"JavaScriptFunction","parameters":{"functionScript":"\r\nconst id = Inputs.Id || '';\r\nif (!id) {\r\n    Outputs.Failure();\r\n    return;\r\n}\r\n\r\nconst { data, error } = await Noodl.Variables.supabaselocal\r\n    .from('noi')\r\n    .select('desc')\r\n    .eq('id', id);\r\n\r\nif (error) {\r\n    Outputs.Error = error.message;\r\n    Outputs.Failure();\r\n} else {\r\n    Outputs.Description = data[0]?.desc || '';\r\n    Outputs.Success();\r\n}\r\n"},"ports":[{"name":"in-Id","displayName":"Id","plug":"input","type":"*","group":"Inputs","index":4},{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":5},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":6},{"name":"out-Error","displayName":"Error","plug":"output","type":"*","group":"Outputs","index":7},{"name":"out-Description","displayName":"Description","plug":"output","type":"*","group":"Outputs","index":8}],"children":[]},{"id":"f6429eec-8a99-4fa7-c4a1-7a783e2ed72a","type":"Model2","parameters":{"modelId":"currentUser"},"ports":[],"children":[]},{"id":"4f2fa705-a57e-7865-e530-3330518251e5","type":"Component Outputs","parame